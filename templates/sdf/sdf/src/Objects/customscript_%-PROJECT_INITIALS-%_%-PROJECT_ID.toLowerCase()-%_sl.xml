<suitelet scriptid="customscript_<%=PROJECT_INITIALS%>_<%=PROJECT_ID.toLowerCase()%>_sl">
  <description></description>
  <isinactive>F</isinactive>
  <name><%=PROJECT_INITIALS.toUpperCase()%> <%=PROJECT_ID.toUpperCase()%> SL</name>
  <notifyadmins>F</notifyadmins>
  <notifyemails></notifyemails>
  <notifyowner>T</notifyowner>
  <notifyuser>F</notifyuser>
  <scriptfile>[/SuiteScripts/<%=FILE_CAB_FOLDER%>/scripts/suitelets/<%=PROJECT_INITIALS%>_<%=PROJECT_ID.toLowerCase()%>_sl.js]</scriptfile>
    <scriptcustomfields>
    <scriptcustomfield scriptid="custscript_<%=PROJECT_ID.toLowerCase()%>_show_nav">
      <accesslevel>2</accesslevel>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <defaultselection></defaultselection>
      <defaultvalue></defaultvalue>
      <description>Checking this box will display the netsuite navbar with the SPA</description>
      <displayheight></displayheight>
      <displaytype>NORMAL</displaytype>
      <displaywidth></displaywidth>
      <dynamicdefault></dynamicdefault>
      <fieldtype>CHECKBOX</fieldtype>
      <help></help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <label>Show Netsuite Nav</label>
      <linktext></linktext>
      <maxlength></maxlength>
      <maxvalue></maxvalue>
      <minvalue></minvalue>
      <onparentdelete></onparentdelete>
      <searchlevel>2</searchlevel>
      <selectrecordtype></selectrecordtype>
      <setting></setting>
      <storevalue>T</storevalue>
    </scriptcustomfield>
  </scriptcustomfields>
  <scriptdeployments>
    <scriptdeployment scriptid="customdeploy_<%=PROJECT_INITIALS%>_<%=PROJECT_ID.toLowerCase()%>_sl">
      <allemployees>T</allemployees>
      <allpartners>F</allpartners>
      <allroles>T</allroles>
      <custscript_<%=PROJECT_ID.toLowerCase()%>_show_nav><%=SHOW_NS_NAVBAR%></custscript_<%=PROJECT_ID.toLowerCase()%>_show_nav>
      <audslctrole></audslctrole>
      <eventtype></eventtype>
      <isdeployed>T</isdeployed>
      <isonline>F</isonline>
      <loglevel>DEBUG</loglevel>
      <runasrole></runasrole>
      <status>RELEASED</status>
      <title><%=PROJECT_INITIALS.toUpperCase()%> SL <%=PROJECT_ID.toUpperCase()%></title>
    </scriptdeployment>
  </scriptdeployments>
</suitelet>